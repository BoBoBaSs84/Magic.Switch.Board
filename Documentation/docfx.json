{
	"metadata": [
		{
			"dest": "api",
			"disableDefaultFilter": false,
			"disableGitFeatures": false,
			"src": [
				{
					"files": [
						"**/**.csproj"
					],
					"src": "..",
					"exclude": [ "**.*Tests.csproj" ]
				}
			]
		}
	],
	"build": {
		"cleanupCacheHistory": true,
		"content": [
			{
				"files": [ "api/**.yml", "api/index.md" ]
			},
			{
				"files": [ "toc.yml", "*.md" ]
			}
		],
		"dest": "public",
		"disableGitFeatures": false,
		"globalMetadata": {
			"_appFaviconPath": "images/favicon.ico",
			"_appFooter": "Copyright Â© BoBoBaSs84.",
			"_appLogoPath": "images/logo.png",
			"_appTitle": "Magic.Switch.Board",
			"_enableSearch": "true"
		},
		"keepFileLink": false,
		"markdownEngineName": "markdig",
		"noLangKeyword": false,
		"overwrite": [
			{
				"files": [ "apidoc/**.md" ],
				"exclude": [ "obj/**", "public/**" ]
			}
		],
		"resource": [
			{
				"files": [ "images/**" ]
			}
		],
		"template": [ "default" ]
	}
}
